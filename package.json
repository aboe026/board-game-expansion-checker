{
  "name": "typescript-template",
  "version": "0.1.0",
  "author": "Adam Boe",
  "description": "A launching point for a TypeScript repository",
  "repository": {
    "type": "git",
    "url": "https://github.com/aboe026/typescript-template.git"
  },
  "bugs": {
    "url": "https://github.com/aboe026/typescript-template/issues"
  },
  "engines": {
    "node": ">=20.17.0",
    "npm": ">=10.8.2"
  },
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc && copyfiles src/template.html build",
    "clean": "rimraf build",
    "lint": "eslint . && prettier --check .",
    "lint-fix": "eslint --fix . && prettier --write .",
    "start": "ts-node src/index.ts",
    "update-dependencies": "ts-node script/update-dependencies.ts"
  },
  "dependencies": {
    "dotenv": "16.4.5",
    "envalid": "8.0.0",
    "log4js": "6.9.1",
    "nodemailer": "6.9.15",
    "xml2js": "0.6.2"
  },
  "devDependencies": {
    "@types/node": "22.5.5",
    "@types/nodemailer": "6.4.16",
    "@typescript-eslint/eslint-plugin": "7.4.0",
    "@typescript-eslint/parser": "7.4.0",
    "copyfiles": "2.4.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "ts-node": "10.9.2",
    "typescript": "5.4.3"
  },
  "packageManager": "yarn@4.5.0"
}
